 RowBox[{"Manipulate", "[", 
  RowBox[{
   RowBox[{"Plot", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"10", "*", 
        RowBox[{"Log10", "[", 
         RowBox[{"thresholdSNREve", "[", 
          RowBox[{
           RowBox[{"10", "^", 
            RowBox[{"(", 
             RowBox[{"x", "/", "10"}], ")"}]}], ",", " ", "r"}], "]"}], 
         "]"}]}], ",", " ", "x", ",", " ", 
       RowBox[{"x", "-", 
        RowBox[{"3", "*", "r"}]}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", " ", 
       RowBox[{"-", "5"}], ",", " ", "35"}], "}"}]}], "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"r", ",", " ", "0.01", ",", " ", "2"}], "}"}]}], "]"}]], "Input",
